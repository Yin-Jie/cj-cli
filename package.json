    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.3.2",
    "@types/semver": "^7.5.0",
    "chalk": "^5.3.0",
    "child_process": "^1.0.2",
    "commander": "^11.0.0",
    "download-git-repo": "^3.0.2",
